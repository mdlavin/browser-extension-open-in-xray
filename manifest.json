{

  "manifest_version": 2,
  "name": "Open In AWS X-Ray",
  "version": "1.0",

  "description": "Adds 'Open In X-Ray' links into SumoLogic query results",

  "icons": {
    "48": "icons/open-in-xray-48.png",
    "128": "icons/open-in-xray-128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.sumologic.com/*"],
      "js": ["openSumoResultInXRay.js"]
    }
  ]
}
